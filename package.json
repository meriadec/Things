    "react-dom": "^0.14.7",
    "react-redux": "^4.4.0",
    "react-router": "^2.0.0",
    "react-router-redux": "^3.0.0",
    "redux": "^3.3.1",
    "redux-actions": "^0.9.1",
    "redux-thunk": "^1.0.3",
    "redial": "^0.4.1"
  },
